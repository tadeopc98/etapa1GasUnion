                <form action="/saveClientSuministro" method="POST">
                    <!-- Datos Personales -->
                    <div class="card">
                        <h5 class="card-header text title-2" style="background-color:cornflowerblue">Datos de Cliente</h3>
                        <div class="card-body">
                            <div class="row mb-3">
                                
                                    <div class="col-md-3">
                                        <label for="noCliente" class="control-label mb-1">Número de Cliente</label>
