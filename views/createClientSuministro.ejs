                                    </div>
                                <div class="col-md-3">
                                    <label for="usuarioReg" class="control-label mb-1">Usuario que registro</label>
                                    <input type="text" class="form-control" id="usuarioReg" name="usuarioReg" value="<%= datos.nombre %>" readonly>
                                </div>
                                <div class="col-md-3">
                                    
                                </div>
                                <div class="col-md-3">
                                    <label for="status" class=" form-control-label">Estatus Cliente</label>
                                    <select name="status" id="status" class="form-control">
                                            <option>ACTIVO</option>
                                            <option>DESACTIVADO</option>
                                    </select>
                                </div>
                                <div class="col-md-6">
                                    <label for="nombre" class="control-label mb-1">Nombre Completo</label>
                                    <input type="text" class="form-control" id="nombre" name="nombre">
                                </div>
                                <div class="col-md-6">
                                    <label for="razonSocial" class="control-label mb-1">Razón Social</label>
                                    <input type="text" class="form-control" id="razonSocial" name="razonSocial">
                                </div>
                                <div class="col-md-12">
                                    <label for="observaciones" class="control-label mb-1">Observaciones</label>
                                    <input type="text" class="form-control" id="observaciones" name="observaciones">
                                </div>
                                <div class="col-md-3">
                                    <label for="telefono" class="control-label mb-1">Teléfono Celular</label>
                                    <input type="text" class="form-control" id="telefono" name="telefono">
                                </div>
                                <div class="col-md-3">
                                    <label for="telFijo" class="control-label mb-1">Telefono Fijo</label>
                                    <input type="text" class="form-control" id="telFijo" name="telFijo">
                                </div>
                                <div class="col-md-3">
                                    <label for="correo" class="control-label mb-1">Correo</label>
                                    <input type="email" class="form-control" id="correo" name="correo">
                                </div>
                                <div class="col-md-3">
                                    <label for="correo2" class="control-label mb-1">Correo Alternativo</label>
                                    <input type="email" class="form-control" id="correo2" name="correo2">
                                </div>
                            </div>
                        </div>
                    </div>
                    <!-- Datos de Dirección -->
                    <div class="card">
                        <h5 class="card-header text title-2" style="background-color:cornflowerblue">Datos de Direccón</h3>
                        <div class="card-body">
                            <div class="row mb-3">
                                <div class="col-md-8">
                                    <label for="calle" class="control-label mb-1">Calle</label>
                                    <input type="text" class="form-control" id="calle" name="calle">
                                </div>
                                <div class="col-md-2">
                                    <label for="noExterior" class="control-label mb-1">Número Exterior</label>
                                    <input type="text" class="form-control" id="noExterior" name="noExterior">
                                </div>
                                <div class="col-md-2">
                                    <label for="noInterior" class="control-label mb-1">Número Interior</label>
                                    <input type="text" class="form-control" id="noInterior" name="noInterior">
                                </div>
                                <div class="col-md-3">
                                    <label for="cp" class="control-label mb-1">Código Postal</label>
                                    <input type="text" class="form-control" id="cp" name="cp">
                                </div>
                                <div class="col-md-3">
                                    <label for="colonia" class="control-label mb-1">Colonia</label>
                                    <input type="text" class="form-control" id="colonia" name="colonia">
                                </div>
                                <div class="col-md-3">
                                    <label for="municipio" class="control-label mb-1">Municipio/Alcaldía</label>
                                    <input type="text" class="form-control" id="municipio" name="municipio">
                                </div>
                                <div class="col-md-3">
                                    <label for="estado" class="control-label mb-1">Estado</label>
                                    <input type="text" class="form-control" id="estado" name="estado">
                                </div>
                            </div>
                        </div>
                    </div>
                    
                    <div class="card">
                        <h5 class="card-header text title-2" style="background-color:cornflowerblue">Datos de Suministro</h3>
                        <div class="card-body">
                            <div class="row mb-3">
                                <div class="col-md-3">
                                    <label for="frecCarga" class=" form-control-label">Frecuencia de carga</label>
                                    <select name="frecCarga" id="frecCarga" class="form-control">
                                            <option>SEMANAL</option>   
                                            <option>QUINCENAL</option>            
                                            <option>MENSUAL</option>
                                            <option>NO RECURRENTE</option>
                                            <option>OTRO</option>
                                    </select>
                                </div>
                                <div class="col-md-3">
                                    <label for="diaCarga" class="control-label mb-1">Día preferente de carga</label>
                                    <input type="text" class="form-control" id="diaCarga" name="diaCarga">
                                </div>
                                <div class="col-md-3">
                                    <label for="tipoPago" class=" form-control-label">Tipo de pago</label>
                                    <select name="tipoPago" id="tipoPago" class="form-control">
                                            <option>EFECTIVO - CONTADO</option>   
                                            <option>TARJETA - CONTADO</option>
                                            <option>TRANSFERENCIA - CONTADO</option>
                                            <option>EFECTIVO - CREDITO</option>   
                                            <option>TARJETA - CREDITO</option>
                                            <option>TRANSFERENCIA - CREDITO</option>
                                            <option>ADMINISTRACIÓN</option>
                                    </select>
                                </div>
                                <div class="col-md-3">
                                    <label for="tipoCliente" class=" form-control-label">Tipo Cliente</label>
                                    <select name="tipoCliente" id="tipoCliente" class="form-control">
                                        <option>CASA - HOGAR</option>   
                                        <option>NEGOCIO - COMERCIO</option>
                                        <option>RESTAURANTE</option>
                                        <option>EDIFICIO SUMINISTRO</option> 
                                    </select>
                                </div>
                                
                            </div>
                        </div>
                    </div>
                    
                    <!-- Botón de Enviar -->
                    <div class="d-flex justify-content-center mt-3">
                        <button type="submit" class="btn btn-outline-success btn-lg mx-2" >Agregar Cliente</button>
                        <a href="/clientesSuministroAdmin" class="btn btn-outline-danger btn-lg mx-2" >Cancelar</a>
                        
                    </div>
                </form>
                <% } %>
