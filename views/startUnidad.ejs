    <script>
        function updateClock() {
            const clockElement = document.getElementById('clock');
            const dateElement = document.getElementById('date');
            const fechaInput = document.getElementById('fechaInicio');
            const timeInput = document.getElementById('horaInicio');
            const now = new Date();
            
            // Formatea la hora
            const hours = now.getHours().toString().padStart(2, '0');
            const minutes = now.getMinutes().toString().padStart(2, '0');
            const seconds = now.getSeconds().toString().padStart(2, '0');
            const timeString = `${hours}:${minutes}:${seconds}`;
            
            // Formatea la fecha
            const year = now.getFullYear();
            const month = (now.getMonth() + 1).toString().padStart(2, '0');
            const day = now.getDate().toString().padStart(2, '0');
            const dateString = `${year}/${month}/${day}`;
            
            // Actualiza los elementos en el HTML
            clockElement.textContent = timeString;
            timeInput.value = timeString;
            dateElement.textContent = dateString;
            fechaInput.value = dateString;

            //Ocultar contenido
            clockElement.style.display = 'none';
            dateElement.style.display = 'none';
        }
        
        // Llama a updateClock inicialmente y luego cada segundo
        updateClock();
        setInterval(updateClock, 1000);
    </script>

